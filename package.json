{
  "name": "herradura",
  "version": "1.0.0",
  "description": "Agro Servicios La Herradura",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\" ",
    "dev": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin Imagenes.html -d dist --htmlmin -o dist/Imagenes.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sngrisa/laherraduraagroservicios.git"
  },
  "author": "Santiago Grisafi",
  "license": "ISC",
  "homepage": "https://github.com/sngrisa",
  "dependencies": {
    "browser-sync": "^2.26.12",
    "browsersync": "0.0.1-security",
    "copyfiles": "^2.3.0",
